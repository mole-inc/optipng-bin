{
	"name": "@mole-inc/optipng-bin",
	"version": "6.0.0",
	"description": "OptiPNG wrapper that makes it seamlessly",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mole-inc/optipng-bin.git"
	},
	"bin": {
		"optipng": "cli.js"
	},
	"engines": {
		"node": ">=10.13.0"
	},
	"scripts": {
		"postinstall": "node lib/install.js",
		"test": "xo && nyc ava",
		"coverage": "nyc report --reporter=lcov",
		"release": "standard-version"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"index.js",
		"cli.js",
		"lib",
		"vendor/source"
	],
	"keywords": [
		"imagemin",
		"compress",
		"image",
		"minify",
		"optimize",
		"png",
		"optipng"
	],
	"dependencies": {
		"@mole-inc/bin-wrapper": "^6.0.0",
		"bin-build": "^3.0.0",
		"bin-version-check": "^4.0.0",
		"logalot": "^2.0.0",
		"which": "^2.0.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"ava": "^3.7.1",
		"bin-check": "^4.1.0",
		"compare-size": "^3.0.0",
		"execa": "^4.0.0",
		"husky": "^4.2.5",
		"nyc": "^15.0.1",
		"standard-version": "^7.1.0",
		"tempy": "^0.5.0",
		"xo": "^0.29.1"
	}
}
